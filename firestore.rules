rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
		match/userdata/{doc}{
    	allow write : if request.auth.uid == doc;
      allow read;
    }
    match/userFriends/{user}{
    	allow write,read : if request.auth.uid == user;
    }
    match/details/{doc}{
    	allow read;
    }
    match/news/{doc}{
    	allow read;
    }
  }
}